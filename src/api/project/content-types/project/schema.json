{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "featured": {
      "type": "boolean",
      "default": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "client": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Creative",
        "Technology",
        "Corporate",
        "Educational",
        "Entertainment"
      ]
    },
    "projectIcon": {
      "type": "enumeration",
      "enum": [
        "Music & Entertainment",
        "Gaming & Technology",
        "Photography & Camera",
        "Innovation & Ideas",
        "Business & Strategy",
        "Education & Learning",
        "Art & Design",
        "Film & Video"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "role": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "shortDescription": {
      "type": "text",
      "required": true
    },
    "challenge": {
      "type": "text"
    },
    "solution": {
      "type": "text"
    },
    "featuredVideo": {
      "type": "component",
      "component": "shared.video-content"
    },
    "featuredImagesTitle": {
      "type": "string"
    },
    "featuredImagesDescription": {
      "type": "text"
    },
    "featuredImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "shortFormVideosTitle": {
      "type": "string"
    },
    "shortFormVideos": {
      "type": "component",
      "component": "shared.video-content",
      "repeatable": true
    },
    "longFormVideosTitle": {
      "type": "string"
    },
    "longFormVideos": {
      "type": "component",
      "component": "shared.video-content",
      "repeatable": true
    },
    "contentStrategy": {
      "type": "component",
      "component": "shared.project-result",
      "repeatable": true
    },
    "productionTechniques": {
      "type": "component",
      "component": "shared.project-result",
      "repeatable": true
    },
    "results": {
      "type": "component",
      "component": "shared.project-result",
      "repeatable": true
    },
    "links": {
      "type": "component",
      "component": "shared.project-result",
      "repeatable": true
    },
    "seo": {
      "type": "component",
      "component": "shared.seo"
    }
  }
}
